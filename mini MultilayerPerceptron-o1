        "import numpy as np\n",
        "\n",
        "def relu(x):\n",
        "   return np.maximum(0, x)\n",
        "\n",
        "x = np.array([[0.4, 0.7], [0.3, 0.1]])\n",
        "\n",
        "w1 = np.array([[0.3, -0.2], [0.5, 0.6]])\n",
        "b1 = np.array([0.0, 0.0])\n",
        "a1 = np.dot(x, w1) + b1\n",
        "z1 = relu(a1)\n",
        "\n",
        "w2 = np.array([[-0.5, 0.1], [-0.1, 0.8]])\n",
        "b2 = np.array([0.0, 0.0])\n",
        "z2 = np.dot(z1, w2) + b2\n",
        "\n",
        "print(\"Output MLP =\", z2)"
      ]
    }
  ]
